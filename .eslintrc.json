{
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "rules": {
        "no-console": "warn",
        "no-alert": "off",
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": "off",
        "max-len": "off",
        "require-jsdoc": "off",
        "padded-blocks": "off",
        "spaced-comment": "off",
        "no-var": "off",
        "object-curly-spacing": "off",
        "no-invalid-this": "off",
        "guard-for-in": "off",
        "new-cap": "off",
        "comma-dangle": "off",
        "no-cond-assign": "error",
        "no-constant-condition": "error",
        "no-control-regex": "error",
        "no-debugger": "error",
        "no-dupe-args": "error",
        "no-dupe-keys": "error",
        "no-duplicate-case": "error",
        "no-empty-character-class": "error",
        "no-empty": "warn",
        "no-ex-assign": "warn",
        "no-extra-boolean-cast": "error",
        "no-extra-parens": "off",
        "no-extra-semi": "error",
        "no-func-assign": "error",
        "no-inner-declarations": "warn",
        "no-invalid-regexp": "error",
        "no-irregular-whitespace": "warn",
        "no-negated-in-lhs": "warn",
        "no-obj-calls": "error",
        "no-regex-spaces": "error",
        "no-sparse-arrays": "error",
        "no-unreachable": "error",
        "use-isnan": "error",
        "valid-typeof": "error",
        "block-scoped-var": "warn",
        "dot-notation": "warn",
        "eqeqeq": "warn",
        "no-eq-null": "warn",
        "no-eval": "warn",
        "no-floating-decimal": "warn",
        "no-implied-eval": "warn",
        "no-labels": "warn",
        "no-lone-blocks": "warn",
        "no-delete-var": "warn",
        "no-undef": "warn",
        "no-unused-vars": "error",
        "no-use-before-define": "off",
        "no-mixed-spaces-and-tabs": "warn"
    }
}